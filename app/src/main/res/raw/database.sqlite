CREATE TABLE cuentas (
 _id  INTEGER PRIMARY KEY INTEGER AUTOINCREMENT,
 usuario VARCHAR(32),
 correo VARCHAR(32),
 mdp VARCHAR(32)

 );

 CREATE TABLE medics (
  _id  INTEGER PRIMARY KEY INTEGER AUTOINCREMENT,
  nombre VARCHAR(32),
  desc VARCHAR(256),
  precio VARCHAR(32)

  );

